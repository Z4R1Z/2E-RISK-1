<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="800.0" prefWidth="1440.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.endGame">
   <children>
      <ImageView fitHeight="800.0" fitWidth="1440.0" layoutY="22.0">
         <image>
            <Image url="@ProjeResimler/victory.jpg" />
         </image>
      </ImageView>
      <Pane layoutX="520.0" layoutY="29.0" prefHeight="218.0" prefWidth="393.0" style="-fx-background-color: #000000;" />
      <Button fx:id="quit" layoutX="14.0" layoutY="598.0" mnemonicParsing="false" onAction="#endGame" prefHeight="200.0" prefWidth="175.0" style="-fx-background-color: #000000;">
         <graphic>
            <ImageView fitHeight="202.0" fitWidth="171.0">
               <image>
                  <Image url="@ProjeResimler/quit.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button fx:id="playAgain" layoutX="1200.0" layoutY="573.0" mnemonicParsing="false" onDragDetected="#playAgain" prefHeight="238.0" prefWidth="234.0" style="-fx-background-color: #000000;">
         <graphic>
            <ImageView fitHeight="200.0" fitWidth="150.0">
               <image>
                  <Image url="@ProjeResimler/playAgain.png" />
               </image>
            </ImageView>
         </graphic></Button>
   </children>
</AnchorPane>
